clear; close all; clc;

% Import the data
load('2_17_2018_data.mat');

% Generate figure 
[lim_row, lim_col] = size(robotarium_data);
 

figure; hold on;
for j = 1:(lim_row/5)
    plot(robotarium_data(j,:),robotarium_data(j+1:5:lim_row,:));
end

%legend('limit','1','2','3','4','5','6','7','8');